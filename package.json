{
  "name": "pageparser",
  "version": "1.0.0",
  "description": "Тестовое задание Node.js fullstack",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "server": "nodemon server.js",
    "front": "npm run serve --prefix front",
    "front:install": "npm install --prefix front",
    "front:build": "npm run build --prefix front",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run front\""
  },
  "author": "Alexandr Dryagin",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "config": "^3.3.9",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1"
  }
}
